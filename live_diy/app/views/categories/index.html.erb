<% flash.each do |type, message| %>
  <p><% type %><%= message %></p>
<% end %>

<p><%= link_to "(+) Add Category", new_category_path(@category) %></p>

<% @categories.each do |category| %>
  <p><%= link_to category.name, category_path(category) %>  <%= link_to "  (Edit)", edit_category_path(category) %></p>
  <%= image_tag category.image_url %>
<% end %>
