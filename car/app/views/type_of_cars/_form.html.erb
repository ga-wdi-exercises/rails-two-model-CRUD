<%= form_for ([@car_model, @type_of_car]) do |f| %>

<div>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :model %>
  <%= f.text_field :model %>

  <%= f.label :photo %>
  <%= f.text_field :photo %>

  <%= f.submit %>
</div>

<% end %>
